{
  "name": "fieldflint",
  "version": "1.0.0",
  "description": "Electron multi-window application",
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "config": {
    "forge": {
      "electronMirror": "https://npmmirror.com/mirrors/electron/",
      "electronBuilderBinariesMirror": "https://npmmirror.com/mirrors/electron-builder-binaries/"
    }
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "vue": "^3.5.14",
    "vue-i18n": "^11.1.3",
    "vuetify": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "happy-dom": "^17.4.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^1.2.1",
    "vue-tsc": "^2.2.10"
